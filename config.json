	"outbounds": [
		{
			"tag": "direct",
			"protocol": "freedom"
		},
		{
			"tag": "out_vless",
			"protocol": "vless",
			"streamSettings": {
				"security": "tls",
				"tlsSettings": {
					"allowInsecure": false,
					"serverName": "yourdomain.domain", //改为你自己的域名
					"fingerprint": "chrome"
				}
			},
			"settings": {
				"vnext": [
					{
						"address": "yourdomain.domain", //改为你自己的域名，直接填写ipv4或ipv6地址也可以
						"port": 443,
						"users": [
							{
								"id": "uuid", //填写uuid
								"encryption": "none",
								"flow": "xtls-rprx-vision-udp443"
							}
						]
					}
				]
			}
		},
		{
			"tag": "out_ss",
			"protocol": "shadowsocks",
			"settings": {
